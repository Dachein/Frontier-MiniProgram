@import "/app.wxss"; 

.container {
  padding: var(--space-xl);
  padding-bottom: 60rpx;
  background-color: var(--ui-bg);
  min-height: 100vh;
}

/* Header Area (Vertical Layout, aligned with Image 2) */
.pd-header {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  margin-bottom: var(--space-xl);
}

.pd-title {
  font-size: var(--fs-title);
  font-weight: var(--fw-bold);
  line-height: 1.3;
  color: var(--ui-primary);
  margin-top: 48rpx; /* 增加 50% 左右的顶部间距 (原来是默认 padding) */
  margin-bottom: var(--space-lg);
}

.pd-meta-section {
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  gap: 20rpx;
  margin-bottom: var(--space-md);
}

.pd-badge-container {
  padding-top: 6rpx; /* 微调对齐 Meta 第一行文字 */
  flex-shrink: 0;
}

.pd-badge-square {
  width: 64rpx;
  height: 64rpx;
  border-radius: 12rpx;
  background: transparent;
  border: 2rpx solid var(--ui-border);
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}

.pd-badge-text {
  font-size: 20rpx;
  font-weight: 700;
  letter-spacing: 0.5rpx;
}

/* Color Coding (Sync with PieceCell) */
.pd-badge-PDF { border-color: #ef4444; }
.pd-badge-PDF .pd-badge-text { color: #ef4444; }

.pd-badge-YTB { border-color: #f87171; }
.pd-badge-YTB .pd-badge-text { color: #f87171; }

.pd-badge-WX { border-color: #10b981; }
.pd-badge-WX .pd-badge-text { color: #10b981; }

.pd-badge-DEFAULT { border-color: #3b82f6; }
.pd-badge-DEFAULT .pd-badge-text { color: #3b82f6; }

.pd-meta-content {
  display: flex;
  flex-direction: column;
  justify-content: center;
  flex: 1;
  min-width: 0;
}

.pd-meta-line1 {
  display: block; /* 改为 block 以便支持多行 */
  font-size: var(--fs-caption);
  font-weight: 600;
  color: var(--ui-secondary);
  letter-spacing: 1rpx;
  margin-bottom: 4rpx;
  line-height: 1.4;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; /* 最多两行 */
  overflow: hidden;
}

.pd-meta-label {
  margin-right: var(--space-xs);
  opacity: 0.8;
  display: inline;
}

.pd-meta-source {
  color: var(--ui-secondary);
  display: inline;
}

.pd-meta-divider {
  margin: 0 var(--space-sm);
  opacity: 0.5;
  display: inline;
}

.pd-meta-author {
  color: var(--ui-secondary);
  display: inline;
}

.pd-meta-arrow {
  margin-left: var(--space-sm);
  font-size: var(--fs-label);
  opacity: 0.5;
  display: inline;
}

.pd-meta-time {
  font-size: var(--fs-caption);
  color: var(--ui-secondary);
  font-weight: 500;
  opacity: 0.8;
}

.pd-tags {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  gap: var(--space-md);
  margin-top: var(--space-sm);
}

.pd-tag {
  font-size: var(--fs-caption);
  color: var(--content-link); /* Blue like Image 2 */
  font-weight: 500;
}

/* Dark Mode Overrides for Icons */
@media (prefers-color-scheme: dark) {
  .pd-badge-PDF { border-color: #f87171; }
  .pd-badge-PDF .pd-badge-text { color: #f87171; }
  .pd-badge-YTB { border-color: #fb7185; }
  .pd-badge-YTB .pd-badge-text { color: #fb7185; }
  .pd-badge-WX { border-color: #34d399; }
  .pd-badge-WX .pd-badge-text { color: #34d399; }
  .pd-badge-DEFAULT { border-color: #60a5fa; }
  .pd-badge-DEFAULT .pd-badge-text { color: #60a5fa; }
}

/* Loading State */
.loading-state {
  margin-top: 40rpx;
  text-align: center;
  color: var(--ui-secondary);
}

/* Takeaway List Title */
.section-title {
  margin: 40rpx 0 var(--space-md);
  color: var(--ui-secondary);
  font-size: var(--fs-caption);
  font-weight: 600;
  letter-spacing: 1px;
}

/* iOS Card Style */
.card {
  background: var(--ui-bg-subtle);
  border-radius: var(--radius-md);
  padding: var(--space-xl);
  margin-bottom: var(--space-lg);
  box-shadow: 0 4rpx 20rpx rgba(0, 0, 0, 0.04);
  transition: all 0.3s ease;
  
  /* 增加淡入动画：从底部滑入 + 透明度变化 (对齐 iOS) */
  animation: cardSlideUp 0.5s cubic-bezier(0.16, 1, 0.3, 1) forwards;
}

@keyframes cardSlideUp {
  from {
    opacity: 0;
    transform: translateY(40rpx);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

.card-header {
  display: flex;
  align-items: flex-start;
  margin-bottom: var(--space-md);
}

.index-num {
  font-size: var(--fs-card);
  color: var(--ui-action);
  margin-right: var(--space-md);
  font-weight: var(--fw-bold);
  line-height: 1.4;
  flex-shrink: 0;
}

.card-title {
  font-size: var(--fs-card);
  font-weight: var(--fw-bold);
  color: var(--ui-primary);
  line-height: 1.4;
}

.card-content {
  font-size: var(--fs-card);
  color: var(--content-text);
  line-height: var(--lh-std);
}

/* Streaming Indicator */
.streaming-footer {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: var(--space-xl);
  color: var(--ui-secondary);
  font-size: var(--fs-caption);
  gap: var(--space-sm);
}

.dot-pulse {
  width: 12rpx;
  height: 12rpx;
  background-color: var(--ui-action);
  border-radius: 50%;
  animation: pulse 1.5s infinite;
}

@keyframes pulse {
  0% { transform: scale(0.8); opacity: 0.5; }
  50% { transform: scale(1.2); opacity: 1; }
  100% { transform: scale(0.8); opacity: 0.5; }
}

/* Floating Action Bar */
.action-bar {
  position: fixed;
  bottom: 40rpx;
  left: 32rpx;
  right: 32rpx;
  height: 120rpx;
  background: rgba(255, 255, 255, 0.85);
  backdrop-filter: blur(20px);
  border-radius: 60rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  padding: 0 20rpx;
  box-shadow: 0 8rpx 32rpx rgba(0, 0, 0, 0.08);
  border: 1rpx solid rgba(0, 0, 0, 0.05);
  transform: translateY(200rpx);
  transition: all 0.4s cubic-bezier(0.16, 1, 0.3, 1);
  z-index: 100;
  visibility: hidden;
  opacity: 0;
}

.action-bar.show {
  transform: translateY(0);
  visibility: visible;
  opacity: 1;
}

.action-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  flex: 1;
  height: 100%;
}

.action-label {
  font-size: 20rpx;
  font-weight: 500;
  color: #666;
  margin-top: 4rpx;
}

/* 统一 Icon 容器样式 */
.action-icon {
  width: 48rpx;
  height: 48rpx;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center;
  opacity: 0.8;
}

